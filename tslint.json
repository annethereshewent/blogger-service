{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "radix": false,
        "one-variable-per-declaration": false,
        "triple-equals": false,
        "quotemark": [true, "single"],
        "variable-name": {
            "severity": "warning"
        },
        "max-classes-per-file": false,
        "semicolon": [true, "never"],
        "eofline": true,
        "indent": [true, "spaces", 4],
        "no-consecutive-blank-lines": true,
        "no-console": false
    },
    "jsRules": {},
    "rulesDirectory": []
}
